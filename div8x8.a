; div8x8.a
	!to "div8x8", cbm
	* = $0b00

	!addr dividend = $c00
	!addr divisor = $c01
	!addr quotient = $c02
	!addr remainder = $c03

	;;  prepare dividend and divisor
	lda #$64
	sta dividend
	lda #9
	sta divisor

div8x8:	lda #0
	sta quotient
	ldx #8
	nop
dloop:	asl dividend
	rol
	cmp divisor
	php
	rol quotient
	plp
	bcc nosbc
	sec
	sbc divisor
	nop
nosbc:	dex
	nop
	bne dloop
	sta remainder
	nop
	rts			
